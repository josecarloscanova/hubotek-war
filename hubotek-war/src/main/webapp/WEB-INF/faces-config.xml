<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<converter> 
		<converter-id>inputDateConverter</converter-id> 
		<converter-class>org.hubotek.web.converter.InputDateConverter</converter-class>
	</converter>
	<application>
		<message-bundle>org.hubotek.messages.HubBaseMessages</message-bundle>
		<resource-bundle>
			<base-name>org.hubotek.messages.system_messages</base-name>
			<var>system_messages</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>org.hubotek.messages.ui_messages</base-name>
			<var>ui_messages</var>
		</resource-bundle>
	</application>
	<navigation-rule>
		<from-view-id />
		<navigation-case>
			<from-outcome>back_home</from-outcome>
			<to-view-id>index.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>